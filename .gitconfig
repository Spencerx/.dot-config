[user]
  email = tony@git-pull.com
  name = Tony Narlock
[color]
  diff = auto
  branch = auto
  status = auto
  ui = always
[diff]
  tool = vimdiff
[difftool]
  prompt = false
[push]
  default = simple
[core]
  editor = vim
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludesfile = ~/.gitignore
	quotepath = false
[web]
  browser = google-chrome
