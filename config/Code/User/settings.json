{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": false,
  "cSpell.enableFiletypes": ["!json", "!jsonc", "!yaml"],
  "cSpell.userWords": ["Endopsychic", "Fairbairn", "sovereignity"],
  "docsYaml.telemetry.enableTelemetry": false,
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "editor.cursorStyle": "line",
  "editor.formatOnSave": true,
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.renderControlCharacters": true,
  "editor.tabSize": 8,
  "editor.unicodeHighlight.allowedCharacters": {
    "…": true,
    "”": true,
    "’": true,
    "“": true
  },
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/.ruff_cache": true,
    "**/*.pyc": true
  },
  "files.watcherExclude": {
    "**/.pytest_cache/*/**": true,
    "**/.ruff_cache/*/**": true,
    "**/.mypy_cache/*/**": true,
    "**/bower_components/*/**": true,
    "**/node_modules/*/**": true
  },
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "never",
  "gitlens.views.fileHistory.avatars": false,
  "python.languageServer": "Pylance",
  "python.testing.unittestArgs": ["-v", "-s", "-p", "*test*.py"],
  "python.venvFolders": ["~/.cache/pypoetry/virtualenvs/"],
  "redhat.telemetry.enabled": false,
  "search.exclude": {
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/.ruff_cache": true,
    "dump/": true
  },
  "security.workspace.trust.untrustedFiles": "newWindow",
  "settingsSync.ignoredSettings": ["-python.venvFolders"],
  "telemetry.telemetryLevel": "off",
  "vim.camelCaseMotion.enable": true,
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {},
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.settings.applyToAllProfiles": [
    "cSpell.enableFiletypes",
    "python.venvFolders",
    "editor.defaultFormatter"
  ],
  "workbench.startupEditor": "newUntitledFile",
  "yaml.schemas": {
    "~/.vscode/extensions/docsmsft.docs-yaml-1.0.1/dist/toc.schema.json": "/toc\\.yml/i"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
